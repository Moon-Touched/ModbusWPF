<Window x:Class="ModbusWPF.Views.RealTrend"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ModbusWPF.Views"
        xmlns:oxy="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Title="RealTrend" Height="1080" Width="1920">
    <Grid>
        <ListView x:Name="DataSelecter" Margin="10,10,1616,10"/>
        <oxy:PlotView x:Name="Plot" Model="{Binding PlotModel}" Margin="326,34,76,52"/>
    </Grid>
</Window>
